#include<ctype.h>
#include<stdlib.h>
#include<conio.h>
#include<math.h>
#include<stdio.h>

int stack[20];
int top=-1;

void push(char a);
void pop();
void post_evaluate(char exp[20])

void main() {
int post_exp[20];
printf("\nEnter postfix expression to be evaluated\t");
gets(post_exp)
post_evaluate(post_exp);
}

void push(char a) {
if(top<20)
stack[++top]
else
    printf("\nStack overflow\n");
}

int pop() {
if(top==-1)
printf("Stack Underflow\n");
else {
        return stack[top];
        top--;
}
}

void post_evaluate(char exp[20]) {
int i=0;
int val;
while(i!=';') {
        if(isaplha(exp[i])) {
            printf("\nEnter data to be substituted for %d\t",exp[i]);
            scanf("%d",&val);
            push(val);
            else
            {
                    if(exp[i]=='^')

            }
        }

}
}
